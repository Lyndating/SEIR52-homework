<h1>Pick you number!</h1>
<form action="/secretnumber/results">
    <label>
    Your number:
        <select name="number" id="num">
        <% @numbers.each do |num|%>
            <option value="<%= num %>"><%= num %></option>
        <% end %>
        </select>
    
    
    </label>
    <button>Send your number</button>
</form>
