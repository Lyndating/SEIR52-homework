<%= form_for @building do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name%>
    <%= f.label :year%>
    <%= f.text_field :year%>
    <%= f.label :height%>
    <%= f.text_field :height%>
    <%= f.label :floor%>
    <%= f.text_field :floor%>
    <%= f.label :image%>
    <%= f.text_field :image%>
    <%= f.submit%>
<% end %>