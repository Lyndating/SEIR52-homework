<%= form_for @city do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name%>
    <%= f.label :country%>
    <%= f.text_field :country%>
    <%= f.label :population%>
    <%= f.text_field :population%>
    <%= f.label :metropolitan_area%>
    <%= f.text_field :metropolitan_area%>
    <%= f.label :image%>
    <%= f.text_field :image%>
    <%= f.submit%>
<% end %>