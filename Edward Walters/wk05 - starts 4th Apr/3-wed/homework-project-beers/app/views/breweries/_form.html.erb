<%= form_for @brewery do |f|%>
    <%= f.label :name %>
    <%= f.text_field :name, :required => true %>
    <%= f.label :city %>
    <%= f.text_field :city %>
    <%= f.label :country %>
    <%= f.text_field :country %>
    <%= f.label :year_founded %>
    <%= f.text_field :year_founded %>
    <%= f.label :website, 'Website URL' %>
    <%= f.url_field :website %>
    <%= f.label :photo, 'Photo URL' %>
    <%= f.url_field :photo %>
  
    <%= f.submit %>
<% end %>